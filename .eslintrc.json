{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "import/extensions": "off",
    "no-alert": "off",
    "no-undef": "off",
    "consistent-return": "off"
  }
}
